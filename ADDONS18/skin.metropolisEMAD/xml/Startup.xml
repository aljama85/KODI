<?xml version="1.0" encoding="UTF-8"?>
<window>
	<onload>ReplaceWindow($INFO[System.StartupWindow])</onload>
	<onload condition="System.HasAddon(script.tv.show.next.aired)">RunScript(script.tv.show.next.aired,silent=True)</onload>
	<onload condition="!Skin.HasSetting(autoscroll)">Skin.SetBool(autoscroll)</onload>
	<onload condition="!Skin.HasSetting(autoscrollfast)">Skin.SetBool(autoscrollfast)</onload>
	<onload condition="!Skin.HasSetting(homestretchbackgrounds)">Skin.SetBool(homestretchbackgrounds)</onload>
	<onload condition="!Skin.String(OSDFade)">Skin.SetString(OSDFade,Complete)</onload>
	<onload condition="!Skin.String(OSDFadeTime)">Skin.SetString(OSDFadeTime,5)</onload>
	<onload condition="!Skin.String(languageicons.name)">Skin.SetString(languageicons.name,Language Flags Complete Pack)</onload>
	<onload condition="!Skin.String(LanguageIcons.path)">Skin.SetString(LanguageIcons.path,resource://resource.images.languageflags.completepack/)</onload>
</window>



